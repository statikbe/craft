<div class="invisible flyout ie-hidden" id="flyout">
	<div class="container">
		<div class="absolute top-0 right-0 mt-4 mr-4">
			<button type="button" class="text-2xl cursor-pointer js-flyout-close" tabindex="0">
				{{ icon('clear') }} 
				<span class="sr-only">{{ 'Sluiten'|t }}</span>
			</button>
		</div>
		<div class="mt-6">
			{% include "_site/_snippet/_nav/_language" %}
		</div>
		<div class="mt-12">
			{% include '_site/_snippet/_nav/_flyout' %}
		</div>
	</div>
</div>
<div class="flyout__overlay js-flyout-close"></div>
